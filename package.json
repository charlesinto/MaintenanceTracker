    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.1.6",
    "dotenv": "^6.0.0",
    "eslint": "^4.19.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5"
  },
  "dependencies": {
